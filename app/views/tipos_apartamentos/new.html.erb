<h1>Inserindo um tipo de apartamento</h1>

<% form_for(@tipos_apartamento) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :nome %><br />
    <%= f.text_field :nome %>
  </p>
  <p>
    <%= f.label :descricao, 'Descrição' %><br />
    <%= f.text_area :descricao %>
  </p>
  <p>
    <%= f.label :valor %><br />
    <%= f.text_field :valor %>
  </p>
  <p>
    <%= f.submit "Inserir" %>
  </p>
<% end %>

<%= link_to 'Voltar', tipos_apartamentos_path %>
